{
  "name": "airtop-mcp",
  "version": "1.0.1",
  "description": "MCP server for Airtop integration",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "bin": {
    "airtop-mcp": "./dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "dev": "tsx watch --env-file=.env src/server.ts",
    "start": "node --env-file=.env dist/server.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "airtop",
    "browser",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@airtop/sdk": "^0.1.34",
    "@dotenvx/dotenvx": "^1.39.0",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@types/express": "^5.0.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.23.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  }
}
