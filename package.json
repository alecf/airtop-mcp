{
  "name": "airtop-mcp",
  "version": "1.0.0",
  "description": "MCP server for Airtop integration",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "bin": {
    "airtop-mcp": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "airtop",
    "browser",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@airtop/sdk": "^0.1.34",
    "@modelcontextprotocol/sdk": "^1.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
